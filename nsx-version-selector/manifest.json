{
  "manifest_version": 3,
  "name": "NSX Version Selector",
  "description": "NSX公式ドキュメントにあるバージョン選択プルダウンリストに、3系には4.2、4系には3.2の選択肢を追加します",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://techdocs.broadcom.com/*/vmware-cis/nsx/*"],
      "js": ["nsx.js"]
    }
  ]
}
